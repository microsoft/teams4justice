{
  "name": "api",
  "version": "0.1.0",
  "types": "dist/index.d.ts",
  "main": "dist/main.js",
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:logs": "rm api*.log -f",
    "prebuild": "run-s clean:dist clean:logs",
    "build:tsc": "tsc -b",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:fix": "run-s lint:prettier:fix lint:eslint:fix",
    "lint": "run-s lint:prettier lint:eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^8.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/passport-azure-ad": "^4.3.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@azure/cosmos": "^3.11.5",
    "@azure/eventgrid": "^4.3.0",
    "@azure/identity": "^1.5.0",
    "@azure/storage-blob": "^12.7.0",
    "@nestjs/azure-database": "^2.3.0",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^8.4.7",
    "@nestjs/cqrs": "^8.0.5",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@tfj/shared": "*",
    "@types/luxon": "^2.0.2",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "html-entities": "^2.3.2",
    "luxon": "^2.0.2",
    "node-html-parser": "^4.1.2",
    "passport-azure-ad": "1.4.6",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.2",
    "tslib": "^2.2.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts",
      "!dist/**"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.spec.ts"
    ]
  }
}
